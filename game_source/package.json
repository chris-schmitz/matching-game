{
  "name": "matching-game",
  "version": "1.0.0",
  "description": "A simple image memeory matching game. ",
  "author": "chris-schmitz <schmitz.chris@gmail.com>",
  "main": "electron_base/index.js",
  "private": true,
  "scripts": {
    "dev:web": "node build/dev-server.js",
    "build:web": "BUILD_DESTINATION=web node build/build.js",
    "dev:electron": "webpack --watch --config build/webpack.electron.conf.js & electron . --enable-logging electron_base/index.js",
    "build:electron": "BUILD_DESTINATION=electron webpack --config build/webpack.electron.conf.js",
    "lint": "eslint --ext .js,.vue src",
    "buildAndPack": "npm run build:electron && npm run package",
    "package": "cross-env NODE_ENV=production electron-packager . --platform=all --out='../_packaged_builds' --overwrite=true",
    "buildAndPack:pi": "npm run build:electron && npm run package:pi",
    "package:pi": "cross-env NODE_ENV=production electron-packager . --platform=linux --arch=armv7l --out='../_packaged_builds' --overwrite=true"
  },
  "dependencies": {
    "cross-env": "^4.0.0",
    "vue": "^2.2.2",
    "vue-router": "^2.2.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.4",
    "electron": "^1.6.2",
    "electron-packager": "^8.6.0",
    "electron-reload": "^1.1.0",
    "eslint": "^3.14.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.7.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "node-sass": "^4.5.0",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.1.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.3",
    "semver": "^5.3.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "vue-loader": "^11.1.4",
    "vue-style-loader": "^2.0.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1",
    "webpack-merge": "^2.6.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
